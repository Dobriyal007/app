wb = xlsx_package.workbook
wb.add_worksheet(name: 'Fas Data') do |sheet|
  sheet.add_row ['Serial No.', 'Date', 'Time', 'Line', 'Model', 'Barcode']

  @fas.each_with_index do |fa, index|
    sheet.add_row [
      index + 1,
      fa.date&.strftime('%Y-%m-%d'),
      fa.time&.strftime('%H:%M:%S'),
      fa.Line,
      fa.Model,
      '', # You may need to modify this part based on how you handle barcodes
    ]
  end
end
<!-- app/views/fas/barcodes.html.erb -->

<h1>Barcode Data</h1>

<table>
  <thead>
    <tr>
      <th>Serial No.</th>
      <th>Code</th>
    </tr>
  </thead>
  <tbody>
    <% @fa.barcodes.each_with_index do |barcode, index| %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= barcode.value %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to '<< Back to FA', fas_path %>
